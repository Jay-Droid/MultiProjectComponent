<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jaydroid.component_login_a">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.jaydroid.component_base_a.application.AApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/component_login_a_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/component_base_a_splash_theme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".activity.LoginGitHubActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>